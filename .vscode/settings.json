{
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": {
        "source.organizeImports": true
    },
    "prettier.printWidth": 140,
    "prettier.singleQuote": true,
    "prettier.useTabs": true,
    "prettier.tabWidth": 2,
    "prettier.semi": true,
    "prettier.bracketSpacing": true,
    "prettier.arrowParens": "always"
}